include $(CONTIKI)/apps/webbrowser/Makefile.webbrowser
ifndef PLATFORM_BUILD
  override webbrowser_src = webclient.c http-strings.c http-user-agent-string.c
endif

APPS += irc
include $(CONTIKI)/apps/irc/Makefile.irc
