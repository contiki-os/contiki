      <interface_config>
        org.contikios.cooja.interfaces.Position
        <x>33.1559139496059</x>
        <y>97.5516298932833</y>
        <z>0.0</z>
      </interface_config>
      <interface_config>
        org.contikios.cooja.mspmote.interfaces.MspClock
