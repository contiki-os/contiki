  <plugin>
    org.contikios.cooja.plugins.ScriptRunner
    <plugin_config>
      <script>GENERATE_MSG(0000, "add-sink");&#xD;
GENERATE_MSG(20000, "remove-sink");&#xD;
GENERATE_MSG(160000, "add-sink");&#xD;
&#xD;
noparent = 0&#xD;
&#xD;
TIMEOUT(200000, if(noparent &gt; 2) { log.testOK(); } );&#xD;
&#xD;
while(true) {&#xD;
    YIELD();&#xD;
    if(msg.equals("remove-sink")) {&#xD;
        m = sim.getMoteWithID(3);&#xD;
        sim.removeMote(m);&#xD;
        log.log("removed sink\n");&#xD;
    } else if(msg.equals("add-sink")) {&#xD;
        if(!sim.getMoteWithID(3)) {&#xD;
            m = sim.getMoteTypes()[1].generateMote(sim);&#xD;
            m.getInterfaces().getMoteID().setMoteID(3);&#xD;
            sim.addMote(m);&#xD;
            log.log("added sink\n");&#xD;
         } else {&#xD;
            log.log("did not add sink as it was already there\n");      &#xD;
         }&#xD;
    } else if(msg.startsWith("Pinging")) {&#xD;
        log.log(msg + '\n');&#xD;
    } else if(msg.startsWith("Got ping")) {&#xD;
        log.log(msg + '\n');&#xD;
    } else if(msg.startsWith("No parent")) {&#xD;
        noparent++;&#xD;
        log.log(msg + '\n');&#xD;
    }&#xD;
}</script>
      <active>true</active>
    </plugin_config>
    <width>976</width>
    <z>0</z>
    <height>920</height>
    <location_x>603</location_x>
    <location_y>43</location_y>
  </plugin>
